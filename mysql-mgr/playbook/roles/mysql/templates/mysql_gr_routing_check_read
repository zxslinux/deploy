# default: on
# description: check to see if the node is a viable routing candidate
service mysql_gr_routing_check_write
{
  disable = no
  flags = REUSE
  socket_type = stream
  port = 6447
  protocal = tcp
  wait = no
  user = root
  server = /usr/local/bin/mysql_gr_routing_check.sh
  server_args = 100 read
  log_on_failure += USERID
}